<body>

<div id="boxedWrapper">

<header class="main-header clearfix">
    <div class="container">
        <h1 class="page-title pull-left">购物车模块</h1>
        <ol class="breadcrumb pull-right">
            <% if (!member) { %>
            <li><a href="/signup">登陆</a></li>
            <li><a href="/reg">注册</a></li>
            <% } else { %>
            <li>欢迎您<%=member.name%><li>
            <li><a href="/logout">退出</a></li>
            <% } %>
        </ol>
    </div>
</header>


<section class="content-area bg1">
    <div class="container">

        <ul id="galleryFilters" class="option-set clearfix list-unstyled list-inline">
            <li><a href="#filter=*" class="btn btn-default btn-primary">全部作品</a></li>
            <li><a href="#filter=.web" class="btn btn-default">图书音像</a></li>
            <li><a href="#filter=.identity" class="btn btn-default">日用化妆</a></li>
            <li><a href="#filter=.print" class="btn btn-default">手机电脑</a></li>
            <li><a href="#filter=.life" class="btn btn-default">服装首饰</a></li>
        </ul>

        <div id="galleryContainer" class="clearfix withSpaces col-3">
            <% if (products) { %>
            <% for ( var i = 0; i < products.length; i++){ %>
            <div class="galleryItem identity">
                <article class="portfolio-item">
                    <div class="portfolio-thumbnail">
                        <a href="/detail/<%= products[i]._id %>"><img src="images/content/works-01.jpg" alt=" "></a>
                        <a href="/detail/<%= products[i]._id %>" class="overlay-img"><span class="overlay-ico"><i class="fa fa-plus"></i></span></a>
                    </div>
                    <div class="entry-meta">
                        <span class="cat-links"><a href="/detail/<%= products[i]._id %>"><%=products[i].name%></a>, <a href="#">智能家居</a></span>
                    </div>
                    <h4 class="entry-title"><a href="/detail/<%= products[i]._id %>"><%=products[i].description%></a></h4>
                </article>
                <!-- / portfolio-item -->
            </div>
            <% }%>
            <% } %>
            <% if (products) { %>
            <% for ( var i = 0; i < products.length; i++){ %>
            <div class="galleryItem web">
                <article class="portfolio-item">
                    <div class="portfolio-thumbnail">
                        <a href="/detail/<%= products[i]._id %>"><img src="images/content/works-01.jpg" alt=" "></a>
                        <a href="/detail/<%= products[i]._id %>" class="overlay-img"><span class="overlay-ico"><i class="fa fa-plus"></i></span></a>
                    </div>
                    <div class="entry-meta">
                        <span class="cat-links"><a href="/detail/<%= products[i]._id %>"><%=products[i].name%></a>, <a href="#">智能家居</a></span>
                    </div>
                    <h4 class="entry-title"><a href="/detail"><%=products[i].description%></a></h4>
                </article>
                <!-- / portfolio-item -->
            </div>
            <% }%>
            <% } %>
            <% if (products) { %>
            <% for ( var i = 0; i < products.length; i++){ %>
            <div class="galleryItem print">
                <article class="portfolio-item">
                    <div class="portfolio-thumbnail">
                        <a href="/detail/<%= products[i]._id %>"><img src="images/content/works-01.jpg" alt=" "></a>
                        <a href="/detail/<%= products[i]._id %>" class="overlay-img"><span class="overlay-ico"><i class="fa fa-plus"></i></span></a>
                    </div>
                    <div class="entry-meta">
                        <span class="cat-links"><a href="/detail/<%= products[i]._id %>"><%=products[i].name%></a>, <a href="#">智能家居</a></span>
                    </div>
                    <h4 class="entry-title"><a href="/detail/<%= products[i]._id %>"><%=products[i].description%></a></h4>
                </article>
                <!-- / portfolio-item -->
            </div>
            <% }%>
            <% } %>
            <% if (products) { %>
            <% for ( var i = 0; i < products.length; i++){ %>
